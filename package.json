{
  "name": "car-api",
  "version": "1.0.0",
  "description": "Resfull API to manage cars",
  "main": "./dist/tsc/main.js",
  "author": "Enrique Plaza",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/main.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
