    "build-all": "yarn clean && yarn build && yarn esbuild-node && yarn esbuild-browser"
  },
  "dependencies": {
    "ajv": "^8.6.2",
    "express": "^4.17.1",
    "mongoose": "^6.0.2"
  },
  "devdependencies": {
    "@types/node": "^16.7.5",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "mocha": "^9.1.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
